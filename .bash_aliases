

# Aliases
alias ..='cd ..'
alias open='xdg-open &>/dev/null'
alias ~='cd ~'
alias dev='cd ~/dev/'
alias docs='cd ~/Documents/'
alias aliases='xdg-open &>/dev/null ~/.bash_aliases'
alias src='source ~/.bashrc'
alias map='cd ~/dev/map'
alias faces='cd ~/dev/faces'
alias sol='cd ~/dev/solitude'
alias solitude='cd ~/dev/solitude'

# Git Aliases
alias add='git add'
alias st='git status'
alias stat='git status'
alias co='git checkout'
alias clone='git clone'
alias cl='git clone'
alias cm='git commit -m'
alias com='git commit -m'
alias df='git diff'
alias amend='git commit --amend'
alias reset='git reset'
